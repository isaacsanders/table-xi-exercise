<!DOCTYPE html>
<html>
  <head>
    <title><%= pkg.name %></title>

    <link rel="stylesheet" type="text/css" href="<%= css %>" media="all" />
  </head>
  <body>
    <div class="navbar navbar-fixed-top navbar-inverse clearfix">
      <div class="navbar-inner">
        <div class="container">
          <a class="brand" href="#"><%= pkg.name %></a>
        </div>
      </div>
    </div>

    <br>
    <br>

    <div class="container">
      <div class="row">
        <div class="menu">
        </div>
      </div>
    </div>
    <script type="text/javascript" src="<%= js %>"></script>
    <script type="text/javascript">
      var menu = new TableXIExercise.Collections.Menu();
      var menuView = new TableXIExercise.Views.Menu({ model: menu });
      menu.fetch();
      menuView.render();
    </script>
  </body>
</html>
